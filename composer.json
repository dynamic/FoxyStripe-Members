{
    "name": "dynamic/foxystripe-members",
    "description": "FoxyStripe Members is an add-on to FoxyStripe that allows for easy integration with MemberProfiles module.",
    "keywords": [
        "silverstripe",
        "foxycart",
        "foxystripe",
        "dynamic",
        "ecommerce"
    ],
    "type": "silverstripe-module",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dynamic Inc",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "require": {
        "dynamic/foxystripe": "^4.0",
        "symbiote/silverstripe-memberprofiles": "^4.0",
        "php": ">=5.6",
        "silverstripe/recipe-core": "^4.2"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "process-timeout": 600
    },
    "autoload": {
        "psr-4": {
          "Dynamic\\FoxyStripeMembers\\": "src/",
          "Dynamic\\FoxyStripeMembers\\Test\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "foxystripe-members",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}